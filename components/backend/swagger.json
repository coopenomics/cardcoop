  "info": {
    "title": "Auth Backend",
    "description": "API для авторизации и управления пользователями",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "InitiateRegistrationDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "Email пользователя",
            "example": "user@example.com"
          }
        },
        "required": [
          "email"
        ]
      },
      "CompleteRegistrationDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "Email пользователя",
            "example": "user@example.com"
          },
          "hashKey": {
            "type": "string",
            "description": "Хэш-ключ для завершения регистрации",
            "example": "secureHash123"
          },
          "uuid": {
            "type": "string",
            "description": "Уникальный идентификатор запроса",
            "example": "uuid-123e4567-e89b-12d3-a456-426614174000"
          }
        },
        "required": [
          "email",
          "hashKey",
          "uuid"
        ]
      },
      "UserResponseDto": {
        "type": "object",
        "properties": {
          "accessToken": {
            "type": "string",
            "description": "Токен доступа для авторизации",
            "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
          },
          "refreshToken": {
            "type": "string",
            "description": "Токен для обновления доступа",
            "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
          }
        },
        "required": [
          "accessToken",
          "refreshToken"
        ]
      },
      "InitiateLoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "Email пользователя",
            "example": "user@example.com"
          }
        },
        "required": [
          "email"
        ]
      },
      "CompleteLoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "Email пользователя",
            "example": "user@example.com"
          },
          "hashKey": {
            "type": "string",
            "description": "Хэш-ключ для завершения входа",
            "example": "secureHash123"
          },
          "uuid": {
            "type": "string",
            "description": "Уникальный идентификатор запроса",
            "example": "uuid-123e4567-e89b-12d3-a456-426614174000"
          }
        },
        "required": [
          "email",
          "hashKey",
          "uuid"
        ]
      },
      "IssueCardDto": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "description": "Имя пользователя, которому будет выдана карта",
            "example": "john_doe"
          },
          "encryptedData": {
            "type": "string",
            "description": "Зашифрованные данные, связанные с картой",
            "example": "encryptedData123456abcdef"
          },
          "coopName": {
            "type": "string",
            "description": "Название кооператива",
            "example": "CoopNameXYZ"
          },
          "signature": {
            "type": "string",
            "description": "Цифровая подпись для проверки данных",
            "example": "signatureBase64Encoded12345"
          }
        },
        "required": [
          "username",
          "encryptedData",
          "coopName",
          "signature"
        ]
      },
      "CardResponseDto": {
        "type": "object",
        "properties": {}
      },
      "PrepareShareDataDto": {
        "type": "object",
        "properties": {
          "coopName": {
            "type": "string",
            "description": "Название кооператива",
            "example": "CoopNameXYZ"
          }
        },
        "required": [
          "coopName"
        ]
      },
      "CoopInfoResponseDto": {
        "type": "object",
        "properties": {
          "coopName": {
            "type": "string",
            "description": "Название кооператива",
            "example": "CoopNameXYZ"
          },
          "coopPublicKey": {
            "type": "string",
            "description": "Публичный ключ кооператива",
            "example": "publicKey123456789abcdef"
          }
        },
        "required": [
          "coopName",
          "coopPublicKey"
        ]
      },
      "ShareDataDto": {
        "type": "object",
        "properties": {
          "coopName": {
            "type": "string",
            "description": "Название кооператива",
            "example": "CoopNameXYZ"
          },
          "encryptedData": {
            "type": "string",
            "description": "Зашифрованные данные для передачи",
            "example": "encryptedData123456abcdef"
          }
        },
        "required": [
          "coopName",
          "encryptedData"
        ]
      },
      "ShareDataResponseDto": {
        "type": "object",
        "properties": {
          "ticket": {
            "type": "string",
            "description": "Билет для обмена данными",
            "example": "exchangeTicket12345"
          }
        },
        "required": [
          "ticket"
        ]
      },
      "CoopExchangeTicketDto": {
        "type": "object",
        "properties": {
          "ticket": {
            "type": "string",
            "description": "Билет для обмена кооперативной информацией",
            "example": "exchangeTicket12345"
          }
        },
        "required": [
          "ticket"
        ]
      },
      "CoopJwtResponseDto": {
        "type": "object",
        "properties": {
          "accessToken": {
            "type": "string",
            "description": "JWT токен доступа для взаимодействия с кооперативом",
            "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
          }
        },
        "required": [
          "accessToken"
        ]
      }
    }
  }
}